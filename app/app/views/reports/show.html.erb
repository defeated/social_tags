<p style="color: green"><%= notice %></p>

<%= render @report %>

<% if @parsed %>
  <p>Open Graph Tags</p>
  <dl>
    <% @parsed[:ogs].each do |(key, value)| %>
      <dt><%= key %></dt>
      <dd><%= value %></dd>
    <% end %>
  </dl>

  <p>Twitter Tags</p>
  <dl>
    <% @parsed[:tws].each do |(key, value)| %>
      <dt><%= key %></dt>
      <dd><%= value %></dd>
    <% end %>
  </dl>
<% end %>

<div>
  <%= link_to "Edit this report", edit_report_path(@report) %> |
  <%= link_to "Back to reports", reports_path %>

  <%= button_to "Destroy this report", @report, method: :delete %>
</div>
